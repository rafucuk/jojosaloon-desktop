{
  "name": "jojosaloon-desktop",
  "version": "1.3.4",
  "description": "A modern and sleek chat app.",
  "main": "src/main.js",
  "type": "module",
  "repository": "https://github.com/rafucuk/jojosaloon-desktop",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --watch * --exec electron .",
    "publish-win": "electron-builder --win -p always",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "SupertigerDev",
  "license": "ISC",
  "devDependencies": {
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.1.0"
  },
  "build": {
    "appId": "jojosaloon.com",
    "productName": "JoJoSaloon",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Network",
      "icon": "build/icon.png"
    },
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "files": [
      "**/*",
      "build/icon.ico",
      "build/icon.png",
      "build/notification_icon.ico",
      "build/notification_icon.png"
    ]
  },
  "dependencies": {
    "@nerimity/active-window-listener": "^2.0.4",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.1.8",
    "portfinder": "^1.0.32",
    "ws": "^8.16.0"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
}
